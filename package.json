    "postcss-syntax": "0.36.2",
    "prettier": "2.8.8",
    "stylelint": "15.8.0",
    "stylelint-config-idiomatic-order": "9.0.0",
    "stylelint-config-standard": "33.0.0",
    "vitest": "0.32.2"
  },
  "lint-staged": {
    "*.css": "stylelint --fix",
    "*.--check": "prettier --write"
