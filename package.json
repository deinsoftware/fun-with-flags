    "eslint-config-next": "13.4.7",
    "next": "13.4.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.1.3"
  },
  "swpm": "npm",
  "volta": {
    "node": "18.16.0",
    "npm": "9.5.1"
  },
  "devDependencies": {
    "@testing-library/dom": "9.3.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
